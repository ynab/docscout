{
  "name": "docscout",
  "version": "1.0.2",
  "description": "NodeJS client for HelpScout Docs API",
  "keywords": [
    "helpscout",
    "helpscout-docs",
    "nodejs",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/ynab/docscout",
  "author": "You Need a Budget, LLC",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc && yarn copy:declarations",
    "copy:declarations": "cp -R ./src/types ./dist/types",
    "release": "ts-node scripts/release.ts",
    "test-run": "ts-node -r dotenv/config scripts/test-run.ts"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.2",
    "dotenv": "^8.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "form-data": "^2.5.0",
    "got": "^11.8.0",
    "lodash": "^4.17.15"
  }
}
